    "webpack-cli": "^3.3.12",
    "yup": "^0.29.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./scripts/jestSetup.js"
    ],
    "testURL": "http://localhost/",
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"

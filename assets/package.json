{
  "private": true,
  "devDependencies": {
    "@testing-library/react": "11.0.4",
    "babel-jest": "^26.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "7.12.1",
    "eslint-config-airbnb": "^18.2.0",

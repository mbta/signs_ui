{
  "private": true,
  "devDependencies": {
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.5.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.4",
    "rc-tabs": "^11.10.1",
    "regenerator-runtime": "^0.13.7"
  },
  "scripts": {
    "deploy": "webpack --mode production",
    "lint": "eslint js/** --fix --ext .js,.jsx,.ts,.tsx",
    "lint:check": "eslint js/** --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint:check && npm run format:check",
    "format": "prettier --write \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"{.,**}/*.{js,json,ts,tsx,css,scss}\"",
    "start": "npm run watch",
    "test": "node scripts/tester.js",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^2.9.11",
    "@sentry/browser": "^6.4.0",
    "@sentry/react": "^6.4.0",
    "@types/dateformat": "^5.0.0",
    "@types/jest": "^29.4.2",
    "@types/phoenix": "^1.5.5",
    "@types/react": "^16.9.56",
    "@types/react-datepicker": "^4.10.0",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "dateformat": "^5.0.3",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^2.7.3",
    "phoenix": "^1.7.1",
    "phoenix_html": "^3.3.1",
    "rc-collapse": "^3.5.2",
    "react": "^16.4.1",
    "react-datepicker": "^4.10.0",
    "react-dom": "^16.4.1",
    "react-hook-form": "^7.43.5",
    "source-map-loader": "^4.0.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "yup": "^0.32"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "setupFilesAfterEnv": [
      "./scripts/jest-setup.ts"
    ],
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(j|t)sx?$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(dateformat)/)"
    ]
  }
}

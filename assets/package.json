    "@types/dateformat": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.23",
    "@types/phoenix": "^1.6.7",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "classnames": "^2.5.1",
    "dateformat": "^5.0.3",
    "lodash": "^4.17.23",
    "phoenix": "^1.8.3",
    "phoenix_html": "^4.3.0",
    "rc-collapse": "^4.0.0",
    "react": "^19.2.3",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.4",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.3",
    "yup": "^1.7"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "setupFilesAfterEnv": [
      "./scripts/jest-setup.ts"
    ],
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(j|t)sx?$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(dateformat)/)"
    ]
  }

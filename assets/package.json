    "@types/dateformat": "^3.0.1",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^29.4.0",
    "@types/phoenix": "^1.5.0",
    "@types/react": "^16.9.56",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-dom": "^16.9.9",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^5.2.7",
    "dateformat": "^4.5.1",
    "formik": "^2.2.1",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.6.2",
    "phoenix": "^1.5.6",
    "phoenix_html": "^2.10.4",
    "rc-collapse": "^3.1.1",
    "react": "^16.4.1",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.4.1",
    "source-map-loader": "^1.1.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "yup": "^1.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "setupFilesAfterEnv": [
      "./scripts/jest-setup.ts"
    ],
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(j|t)sx?$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    }
  }
}

    "regenerator-runtime": "^0.14.1",
    "resize-observer-polyfill": "^1.5.1",
    "typescript-eslint": "^8.50.0"
  },
  "scripts": {
    "deploy": "node build.mjs deploy",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "typecheck": "tsc --noEmit",
